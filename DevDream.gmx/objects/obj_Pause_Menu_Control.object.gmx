        interest = "quit";
    }
    else if (interest == "quit")
    {
        interest = "main menu";
    }
    else if (interest == "main menu")
    {
        interest = "options";
    }
    else if (interest == "options")
    {
        interest = "load";
    }
    else if (interest == "load")
    {
        interest = "save";
    }
    else if (interest == "save")
    {
        interest = "resume";
    }
    ///////////////////////////////
}

if (control_down)
{
    ///////////////////////////////
    if (interest == "resume")
    {
        interest = "save";
    }
    else if (interest == "save")
    {
        interest = "load";
    }
    else if (interest == "load")
    {
        interest = "options";
    }
    else if (interest == "options")
    {
        interest = "main menu";
    }
    else if (interest == "main menu")
    {
        interest = "quit";
    }
    else if (interest == "quit")
    {
        interest = "resume";
    }
    ///////////////////////////////
}

if (control_escape || control_b)
{
    ///////////////////////////////
    instance_destroy();
    ///////////////////////////////
}

if (control_enter)
{
    ///////////////////////////////
    if (interest == "resume")
    {
        instance_destroy();
    }
    else if (interest == "save")
    {
        show_message("Coming soon");
    }
    else if (interest == "load")
    {
        show_message("Coming soon");
    }
    else if (interest == "options")
    {
        show_message("Coming soon");
    }
    else if (interest == "main menu")
    {
        room_goto(rm_Main_Menu);
    }
    else if (interest == "quit")
    {    
        game_end();
    }
    ///////////////////////////////
}

